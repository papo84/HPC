let scrapePage = require('./openArticleInvoker').scrapePage;
let db = require('../db/mongoClient');
db.start();
scrapeHoops();

async function scrapeHoops() {
    for (let i = 1; i < 20; i++) {
        let url = `http://www.hoops.co.il/?paged=${i}`;
        console.log(`scraping ${i}`);
        await scrapePage({ url: url });
    }

}